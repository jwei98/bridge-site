<h1><%= @title %></h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Sign in At</th>
      <th>Created</th>
      <th>Admin?</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.last_sign_in_at.strftime("%m-%d-%Y @ %H:%M:%S") %></td>
        <td><%= user.created_at.strftime("%m-%d-%Y") %></td>
        <td><%= user.admin %></td>
        <td><%= link_to 'Show', user %> <%= link_to 'Edit', edit_user_path(user) %> <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>